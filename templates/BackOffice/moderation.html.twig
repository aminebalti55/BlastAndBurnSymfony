                            <h6 class="mb-0 text-sm">{{ all.title }}</h6>
                            <p class="text-xs text-secondary mb-0">{{ all.type | upper }}</p>
                          </div>
                        </div>
                      </td>
                      <td>
                        <p class="text-xs font-weight-bold mb-0">{{ all.reporter.firstName ~ ' ' ~ all.reporter.lastName}}</p>
                        <p class="text-xs text-secondary mb-0">{{ all.reporter.email }}</p>
                      </td>
                      <td>
                        <p class="text-xs font-weight-bold mb-0">{{ all.note }}</p>
                      </td>
                      <td class="align-middle text-center text-sm">
                        <span class="{{ statusType }}">{{ status }}</span>
                      </td>
                      <td class="align-middle text-center">
                        <span class="text-secondary text-xs font-weight-bold">{{ all.createdAt | date('d/m/Y  H:m:s') }}</span>
                      </td>
                      <td class="align-middle">
                        {% if all.isClosed == '0' %}
                        <a href="{{ path('close_report', {'reportId':all.reportId}) }}" class="text-primary font-weight-bold text-sm-center">
                          Close
                        </a>
                          {% else %}
                            <a class="cursor text-muted font-weight-bold text-sm-center">
                              Closed
                            </a>
                        {% endif %}
                      </td>
                    </tr>
